Config { overrideRedirect = False
       , font     = "9x15" --"xft:Terminus:size=14"
       , bgColor  = "#000000"
       , fgColor  = "#ffffff"
       , position = TopW L 100
       , commands = [ Run Cpu ["-L","3","-H","50","-h","red","-n","green","-t","<total>","-S","True","-p","3"] 50
                    , Run Memory ["-L","3","-H","50","-h","red","-n","green","-t","<usedratio>","-S","True","-p","3"] 50
                    , Run Date "%d/%m  %H:%M" "date" 10
                    , Run XMonadLog
		    , Run Battery ["-t","<leftipat> <acstatus>","-L","20","-H","70","-l","red","-n","yellow","-h","green",
                                  "--","-o","<left>","-O","<left>","-i","<left>","--on-icon-pattern","<icon=/home/bemkap/.icons/xmobar/battery_on_%%.xpm/>",
				  "--off-icon-pattern","<icon=/home/bemkap/.icons/xmobar/battery_off_%%.xpm/>","--idle-icon-pattern","<icon=/home/bemkap/.icons/xmobar/battery_idle_%%.xpm/>"] 50
                    , Run Wireless "wlp3s0" ["-t","<ssid> <quality>","-h","green","-n","yellow","l","red","-S","True","-p","3"] 50
                    , Run K10Temp "0000:00:18.3" ["-t","<Tctl>","-L","40","-H","70","-l","lightblue","-n","green","-h","red","-S","True"] 50
                    , Run Com "/home/bemkap/.xmonad/bluetooth.pl" [] "btctl" 50
                    ]
       , sepChar  = "%"
       , alignSep = "}{"
       , template = "%XMonadLog% }{ <icon=/home/bemkap/.icons/xmobar/bluetooth.xpm/> %btctl% | %wlp3s0wi% | <icon=/home/bemkap/.icons/xmobar/cpu_0.xpm/> %cpu% | %battery% | <icon=/home/bemkap/.icons/xmobar/temperature_0.xpm/> %k10temp% | <icon=/home/bemkap/.icons/xmobar/ram_0.xpm/> %memory% | %date% "
       }
