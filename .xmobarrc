Config { overrideRedirect = False
       , font     = "xft:Terminus:size=14"
       , bgColor  = "#000000"
       , fgColor  = "#ffffff"
       , position = TopW L 100
       , commands = [ Run Cpu ["-L","3","-H","50","-h","red","-n","green","-t","<total>%","-p","3"] 10
                    , Run Memory ["-L","3","-H","50","-h","red","-n","green","-t","<usedratio>","-S","True","-p","3"] 10
                    , Run Date "%d %m %Y   %H:%M" "date" 10
                    , Run XMonadLog
		    , Run Battery ["-t","<acstatus>","-L","10","-H","80","-l","darkred","-n","darkorange","-h","darkgreen"
                                  ,"--","-o","-<left>%","-O","+<left>","-i","F<left>"] 10
                    , Run Wireless "wlp3s0" ["-t","<ssid> <quality>","-h","green","-n","yellow","l","red","-S","True"] 50
                    , Run K10Temp "0000:00:18.3" ["-t","<Tctl>","-L","40","-H","70","-l","lightblue","-n","green","-h","red","-S","True"] 10
                    ]
       , sepChar  = "%"
       , alignSep = "}{"
       , template = "%XMonadLog% }{ %wlp3s0wi% | C %cpu% | %battery% | T %k10temp% | M %memory% | %date%         "
       }
