{
    "manifest_version": 2,
    "name": "anime-list",
    "version": "0.01",
    "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
    },
    "background": {
	"scripts": ["background.js"]
    },
    "content_scripts": [{
	"matches": ["http://animeflv.net/ver/*"],
	"js": ["anime.js"]
    }],
    "permissions": [
	"tabs",
	"http://animeflv.net/ver/*",
	"storage"
    ]
}
