{
    "manifest_version": 2,
    "name": "anime-list",
    "version": "0.2",
    "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
    },
    "background": {
	"scripts": ["background.js"]
    },
    "commands": {
	"save-anime": {
	    "suggested_key": {
		"default": "Ctrl+Shift+A"
	    },
	    "description": "read the name"
	}
    },
    "permissions": [
	"tabs",
        "storage",
    ]
}
